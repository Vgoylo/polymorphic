<div class="container">
  <div class="row">
    <div class="col-md-7">
      <%= link_to 'Back', clients_path, class: 'btn btn-primary' %></td>
      <%= form_for @client do |f| %>
        <div class="field">
          <%= f.label :first_name %><br/>
          <%= f.text_field :first_name, required: true, class: "form-control" %>
        </div>
         <div class="field">
          <%= f.label :last_name %><br/>
          <%= f.text_field :last_name, required: true, class: "form-control" %>
        </div>
         <div class="field">
          <%= f.label :phone %><br/>
          <%= f.text_field :phone, required: true, class: "form-control" %>
        </div>
         <div class="field">
          <%= f.label :email %><br/>
          <%= f.text_field :email, required: true, class: "form-control" %>
        </div>

        <div class="actions">
          <%= f.submit 'Save', class:"btn btn-primary" %>
        </div>
      <% end %>
    </div>
    <div class="col"></div>
  </div>
</div>

